    "constraints": [
      "CONSTRAINT\\s+[`'\"]?(\\w+)[`'\"]?",
      "UNIQUE\\s*\\([`'\"]?(\\w+)[`'\"]?",
      "CHECK\\s*\\([^)]+\\)",
      "NOT\\s+NULL",
      "DEFAULT\\s+"
    ],
    "schemas": [
      "CREATE\\s+SCHEMA\\s+(?:IF\\s+NOT\\s+EXISTS\\s+)?[`'\"]?(\\w+)[`'\"]?",
      "DROP\\s+SCHEMA\\s+(?:IF\\s+EXISTS\\s+)?[`'\"]?(\\w+)[`'\"]?",
      "USE\\s+[`'\"]?(\\w+)[`'\"]?",
      "[`'\"]?(\\w+)[`'\"]?\\.[`'\"]?(\\w+)[`'\"]?"
    ],
    "sqlite_specific": [
      "PRAGMA\\s+(\\w+)",
      "ATTACH\\s+DATABASE\\s+['\"]([^'\"]+)['\"]",
      "DETACH\\s+DATABASE\\s+[`'\"]?(\\w+)[`'\"]?",
      "VACUUM",
      "REINDEX",
      "ANALYZE\\s+[`'\"]?(\\w+)[`'\"]?"
    ],
    "embedded_sql_patterns": [
      "execute\\(['\"]\\s*(SELECT\\s+.*?FROM\\s+(\\w+).*?)['\"]",
      "execute\\(['\"]\\s*(INSERT\\s+INTO\\s+(\\w+).*?)['\"]",
      "execute\\(['\"]\\s*(UPDATE\\s+(\\w+).*?)['\"]",
      "execute\\(['\"]\\s*(DELETE\\s+FROM\\s+(\\w+).*?)['\"]",
      "execute\\(f['\"]\\s*(SELECT\\s+.*?FROM\\s+(\\w+).*?)['\"]",
      "execute\\(f['\"]\\s*(INSERT\\s+INTO\\s+(\\w+).*?)['\"]",
      "execute\\(f['\"]\\s*(UPDATE\\s+(\\w+).*?)['\"]",
      "execute\\(f['\"]\\s*(DELETE\\s+FROM\\s+(\\w+).*?)['\"]",
      "executescript\\(['\"]([^'\"]+)['\"]",
      "\\?\\s*,\\s*\\(",
      "\\(\\s*\\?\\s*(?:,\\s*\\?)*\\s*\\)"
    ]
  }
