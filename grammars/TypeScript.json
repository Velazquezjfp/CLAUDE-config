{
  "language": "typescript",
  "version": "2.0",
  "last_updated": "2025-12-16",
  "description": "TypeScript grammar patterns for code graph extraction",
  "patterns": {
    "imports": [
      "^\\s*import\\s+.*\\s+from\\s+['\"]([^'\"]+)['\"]",
      "^\\s*import\\s+['\"]([^'\"]+)['\"]",
      "^\\s*import\\s+type\\s+.*\\s+from\\s+['\"]([^'\"]+)['\"]",
      "^\\s*import\\s*\\(\\s*['\"]([^'\"]+)['\"]\\s*\\)",
      "^\\s*export\\s+.*\\s+from\\s+['\"]([^'\"]+)['\"]"
    ],
    "exports": [
      "^\\s*export\\s+default\\s+(function|const|class|interface|type)\\s+(\\w+)",
      "^\\s*export\\s+default\\s+(\\w+)",
      "^\\s*export\\s+(function|const|let|var|class|interface|type|enum)\\s+(\\w+)",
      "^\\s*export\\s+\\{([^}]+)\\}",
      "^\\s*export\\s+\\{[^}]*\\}\\s+from\\s+['\"]([^'\"]+)['\"]"
    ],
    "functions": [
      "function\\s+(\\w+)\\s*(?:<[^>]+>)?\\s*\\(",
      "(?:export\\s+)?(?:const|let|var)\\s+(\\w+)\\s*=\\s*(?:async\\s+)?\\([^)]*\\)\\s*(?::\\s*[^=]+)?\\s*=>",
      "(?:export\\s+)?(?:const|let|var)\\s+(\\w+)\\s*=\\s*(?:async\\s+)?function",
      "async\\s+function\\s+(\\w+)\\s*\\(",
      "(?:public|private|protected)?\\s*(\\w+)\\s*\\([^)]*\\)\\s*(?::\\s*\\w+)?\\s*\\{"
    ],
    "classes": [
      "class\\s+(\\w+)(?:<[^>]+>)?(?:\\s+extends\\s+\\w+(?:<[^>]+>)?)?(?:\\s+implements\\s+[\\w,\\s<>]+)?\\s*\\{"
    ],
    "interfaces": [
      "interface\\s+(\\w+)(?:<[^>]+>)?(?:\\s+extends\\s+[\\w,\\s<>]+)?\\s*\\{"
    ],
    "types": [
      "type\\s+(\\w+)(?:<[^>]+>)?\\s*="
    ],
    "enums": [
      "enum\\s+(\\w+)\\s*\\{"
    ],
    "constants": [
      "(?:export\\s+)?const\\s+(\\w+)\\s*=",
      "(?:export\\s+)?const\\s+(\\w+)\\s*:\\s*[^=]+\\s*="
    ],
    "api_endpoints": [
      "(?:app|router|express)\\.(get|post|put|delete|patch|use)\\s*\\(['\"]([^'\"]+)['\"]",
      "@(Get|Post|Put|Delete|Patch)\\(['\"]([^'\"]+)['\"]\\)",
      "@Controller\\(['\"]([^'\"]+)['\"]\\)",
      "\\.route\\(['\"]([^'\"]+)['\"]\\)",
      "fetch\\(['\"]([^'\"]+)['\"]",
      "axios\\.(get|post|put|delete|patch)\\(['\"]([^'\"]+)['\"]"
    ],
    "database_queries": [
      "\\.find\\(\\{",
      "\\.findOne\\(\\{",
      "\\.findById\\(",
      "\\.create\\(",
      "\\.update\\(",
      "\\.delete\\(",
      "\\.query\\(",
      "SELECT\\s+.*\\s+FROM\\s+(\\w+)",
      "INSERT\\s+INTO\\s+(\\w+)",
      "UPDATE\\s+(\\w+)\\s+SET",
      "DELETE\\s+FROM\\s+(\\w+)",
      "@Entity\\(['\"]?([^'\"\\)]+)['\"]?\\)",
      "getRepository\\((\\w+)\\)",
      "prisma\\.(\\w+)\\."
    ],
    "decorators": [
      "@(\\w+)(?:\\([^)]*\\))?"
    ]
  }
}
