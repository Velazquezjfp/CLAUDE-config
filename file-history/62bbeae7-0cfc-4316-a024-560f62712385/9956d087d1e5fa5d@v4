    <script>
        function toggleCard(card) {
            card.classList.toggle('expanded');
        }

        function expandAll() {
            document.querySelectorAll('.table-card').forEach(card => {
                card.classList.add('expanded');
            });
        }

        function collapseAll() {
            document.querySelectorAll('.table-card').forEach(card => {
                card.classList.remove('expanded');
            });
        }
    </script>
